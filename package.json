{
  "name": "vn-addresskit",
  "version": "0.1.0",
  "description": "Type-safe TypeScript wrapper for AddressKit Vietnam API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vietnam",
    "address",
    "api",
    "typescript",
    "addresskit",
    "provinces",
    "districts",
    "wards",
    "vietnamese",
    "location",
    "geography"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaQuangKhoi/vn-addresskit.git"
  },
  "bugs": {
    "url": "https://github.com/TaQuangKhoi/vn-addresskit/issues"
  },
  "homepage": "https://github.com/TaQuangKhoi/vn-addresskit#readme",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "tsdown": "^0.3.0"
  },
  "dependencies": {
    "whatwg-fetch": "^3.6.20"
  }
}
